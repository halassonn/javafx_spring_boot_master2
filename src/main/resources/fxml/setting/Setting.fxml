<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>
<ScrollPane fx:id="root_setting" fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ideb.idebgtm.controller.setting.SettingController">
   <content>
      <StackPane fx:id="main_Setting">
         <children>

            <FlowPane fx:id="flow_menu" columnHalignment="CENTER" hgap="15.0" vgap="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#open_system_setting" prefHeight="85.0" prefWidth="85.0" styleClass="custom_button" text="SYSTEM" />
                  <Button layoutX="221.0" layoutY="30.0" mnemonicParsing="false" onAction="#open_parameter_setting" prefHeight="85.0" prefWidth="85.0" styleClass="custom_button" text="Parameter" />
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#open_bpjs_setting" prefHeight="85.0" prefWidth="85.0" styleClass="custom_button" text="BPJS" />
                  <Button layoutX="120.0" layoutY="30.0" mnemonicParsing="false" onAction="#open_pph21_setting" prefHeight="85.0" prefWidth="85.0" styleClass="custom_button" text="PPh21" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="10.0" top="20.0" />
               </padding>
            </FlowPane>
         </children>
      </StackPane>
   </content>
</ScrollPane>
